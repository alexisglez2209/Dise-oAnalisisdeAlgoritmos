def fibonacci_diccionario(n):
    # entrada: Un número entero n
    # salida: El n-ésimo número de Fibonacci
    A = {}
    
    if n <= 0:
        return 0
    elif n == 1:
        return 1

    A[0] = 0
    A[1] = 1

    for i in range(2, n + 1):
        A[i] = A[i - 1] + A[i - 2]

    return A[n]

def fibonacci_memorizacion(n, memo={}):
    # entrada: Un número entero n, un diccionario memo
    # salida: El n-ésimo número de Fibonacci
    if n in memo:
        return memo[n]

    if n <= 0:
        return 0
    elif n == 1:
        return 1

    memo[n] = fibonacci_memorizacion(n - 1, memo) + fibonacci_memorizacion(n - 2, memo)
    return memo[n]
